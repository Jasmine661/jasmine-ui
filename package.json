{
  "name": "jasmine661-ui",
  "version": "1.0.7",
  "description": "A React component library",
  "main": "dist/jasmine-ui.umd.js",
  "module": "dist/jasmine-ui.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "eslint-config-jasmine-ui.js",
    "ESLINT.md",
    "README.md"
  ],
  "keywords": [
    "react",
    "ui",
    "components",
    "typescript"
  ],
  "author": "Jasmine661",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jasmine661/jasmine-ui.git"
  },
  "homepage": "https://github.com/Jasmine661/jasmine-ui#readme",
  "bugs": {
    "url": "https://github.com/Jasmine661/jasmine-ui/issues"
  },
  "eslintConfig": {
    "extends": "./eslint-config-jasmine-ui.js"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm run build:types && pnpm run build:css",
    "build:parallel": "pnpm run build:types & pnpm run build:css & vite build",
    "build:analyze": "vite build --mode analyze",
    "build:prod": "cross-env NODE_ENV=production vite build && pnpm run build:types && pnpm run build:css",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "lint:fix": "eslint --ext js,ts,tsx src --fix",
    "lint:check": "eslint --ext js,ts,tsx src --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:nowatch": "vitest run --no-watch ",
    "build:types": "tsc --p tsconfig.build.json",
    "build:css": "sass src/styles/index.scss dist/jasmine661-ui.css",
    "link": "pnpm link --global",
    "unlink": "pnpm unlink --global",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook",
    "prepare": "husky",
    "prepublish": "npm run test:nowatch && npm run lint && npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "async-validator": "^4.2.5",
    "axios": "^1.10.0",
    "classnames": "^2.5.1",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.0.1",
    "@eslint/js": "^9.25.0",
    "@storybook/addon-a11y": "9.0.12",
    "@storybook/addon-docs": "9.0.12",
    "@storybook/addon-onboarding": "9.0.12",
    "@storybook/addon-vitest": "9.0.12",
    "@storybook/react-vite": "9.0.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-transition-group": "^4.4.12",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "3.2.0",
    "@vitest/coverage-v8": "3.2.0",
    "@vitest/ui": "^3.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "9.0.12",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.1",
    "prettier": "^3.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "sass": "^1.89.1",
    "storybook": "9.0.12",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.0"
  }
}
